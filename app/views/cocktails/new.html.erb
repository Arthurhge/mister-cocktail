<div class="col-sm-6 col-md-offset-3">
  <%= bootstrap_form_for(@cocktail, html: { multipart: true }) do |f| %>
    <p>
      <%= f.text_field :name %>
    </p>
    <p>
      <%= f.file_field :picture %>
    </p>
    <p><%= f.submit "Create Cocktail", class: "btn btn-primary" %></p>
  <% end %>
</div>

